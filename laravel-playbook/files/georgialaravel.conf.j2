<VirtualHost *:80>

    ServerName georgialaravel.me
    ServerAlias www.georgialaravel.me
    ServerAdmin admin@georgialaravel.me

    DocumentRoot /var/www/html/georgialaravel/public

    <Directory /var/www/html/georgialaravel>
        Options Indexes MultiViews
        AllowOverride None
        Require all granted
    </Directory>

    ErrorLog ${APACHE_LOG_DIR}/error.log
    CustomLog ${APACHE_LOG_DIR}/access.log combined

</VirtualHost> 
